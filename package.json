{
  "name": "advanced-redux",
  "version": "0.0.1",
  "main": "server/server.js",
  "scripts": {
    "test": "NODE_ENV=test nyc ava",
    "test:watch": "ava --watch",
    "lint": "eslint .",
    "cover": "nyc report --reporter=html",
    "cover:watch": "nodemon --watch test/ --exec 'npm run test && npm run cover'",
    "cover:show": "open coverage/index.html",
    "cover:alls": "nyc report --reporter=text-lcov | coveralls",
    "start": "npm run create-env && gulp",
    "seed": "npm run create-env && node seed",
    "create-env": "node -e \"var fs = require('fs'); fs.access('./.env', function(err) { if (err) { console.log('\\n\\ncreating .env file\\n\\n'); return fs.writeFileSync('./.env', ''); } console.log('\\n\\n.env file present\\n\\n'); });\""
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express-state": "^1.4.0",
    "fetchr": "^0.5.37",
    "helmet": "^1.3.0",
    "lodash": "^4.15.0",
    "loopback": "^2.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^2.4.0",
    "loopback-datasource-juggler": "^2.39.0",
    "morgan": "^1.7.0",
    "normalizr": "^2.2.1",
    "pug": "^2.0.0-beta6",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "^5.0.4",
    "react-redux-epic": "^0.3.1",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-observable": "^0.14.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "rxjs": "^5.4.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^1.0.1"
