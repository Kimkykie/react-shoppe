    "browser-sync": "^2.15.0",
    "eslint": "^3.4.0",
    "eslint-config-loopback": "^4.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0",
    "gulp": "^3.9.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.5.0",
    "gulp-util": "^3.0.7",
    "json-loader": "^0.5.4",
    "kouto-swiss": "^0.12.0",
    "nodemon": "^1.10.2",
    "nyc": "^10.3.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
